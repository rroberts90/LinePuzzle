{
  "expo": {
    "name": "Primaze",
    "slug": "Primaze",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/1024.png",
    "splash": {
      "image": "./assets/splash4.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFAFA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "./assets/",
      "./Icons/",
      "./Sounds/"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rroberts90.ColorMaze"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}
